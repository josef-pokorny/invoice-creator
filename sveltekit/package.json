{
    "name": "sveltekit",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "pnpm run translate && vite build",
        "preview": "vite preview",
        "prepare": "svelte-kit sync || echo ''",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write --config ./.prettierrc.json .",
        "lint": "eslint . && prettier --config ./.prettierrc.json --check .",
        "lint-fix": "eslint --fix . && pnpm run format",
        "translate": "inlang machine translate --project src/project.inlang",
        "postbuild": "npx svelte-sitemap --domain https://invoice-creator.josefpokorny.cz -o .vercel/output/static"
    },
    "devDependencies": {
        "@eslint/compat": "^1.3.0",
        "@eslint/js": "^9.29.0",
        "@fontsource-variable/montserrat": "^5.2.6",
        "@inlang/cli": "^3.0.12",
        "@inlang/paraglide-js": "^2.1.0",
        "@internationalized/date": "^3.8.2",
        "@lucide/svelte": "^0.516.0",
        "@react-pdf/renderer": "^4.3.0",
        "@skeletonlabs/skeleton": "^3.1.3",
        "@skeletonlabs/skeleton-svelte": "^1.2.3",
        "@sveltejs/adapter-node": "^5.2.12",
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/adapter-vercel": "^5.7.2",
        "@sveltejs/kit": "^2.22.0",
        "@sveltejs/vite-plugin-svelte": "^5.1.0",
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/postcss": "^4.1.10",
        "@tailwindcss/vite": "^4.1.10",
        "@vite-pwa/sveltekit": "^1.0.0",
        "axios": "^1.10.0",
        "classnames": "^2.5.1",
        "clsx": "^2.1.1",
        "country-flag-icons": "^1.5.19",
        "eslint": "^9.29.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-perfectionist": "^4.15.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-svelte": "^3.9.3",
        "eslint-plugin-unused-imports": "^4.1.4",
        "globals": "^16.2.0",
        "i18n-iso-countries": "^7.14.0",
        "lodash": "^4.17.21",
        "moment": "^2.30.1",
        "moment-timezone": "^0.6.0",
        "paneforge": "1.0.0-next.6",
        "pdfjs-dist": "4.10.38",
        "postcss": "^8.5.6",
        "prettier": "^3.5.3",
        "prettier-plugin-svelte": "^3.4.0",
        "prettier-plugin-tailwindcss": "^0.6.13",
        "react": "^19.1.0",
        "sass-embedded": "^1.89.2",
        "svelte": "^5.34.7",
        "svelte-check": "^4.2.2",
        "svelte-sitemap": "^2.7.1",
        "tailwind-variants": "^1.0.0",
        "tailwindcss": "^4.1.10",
        "tailwindcss-animate": "^1.0.7",
        "tw-animate-css": "^1.3.4",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.34.1",
        "uuid": "^11.1.0",
        "vite-plugin-circular-dependency": "^0.5.0",
        "vite-plugin-pwa": "^1.0.0",
        "workbox-precaching": "^7.3.0",
        "workbox-window": "^7.3.0",
        "yup": "^1.6.1",
        "yup-locales": "^1.2.28"
    },
    "dependencies": {
        "@emotion/css": "^11.13.5",
        "@josef-pokorny/svelte-pdf": "^1.0.0",
        "@types/country-flag-icons": "^1.2.2",
        "@types/lodash": "^4.17.18",
        "@types/lodash-es": "^4.17.12",
        "@types/react": "^19.1.8",
        "autoprefixer": "^10.4.21",
        "flowbite": "^3.1.2",
        "flowbite-svelte": "1.0.0-next.20",
        "flowbite-svelte-icons": "^2.2.1",
        "melt": "^0.35.1",
        "tailwind-merge": "^3.3.1",
        "vite": "^6.3.5",
        "vite-plugin-devtools-json": "^0.2.0",
        "vite-plugin-mkcert": "^1.17.8"
    }
}
